{
  "name": "google-maps-api-stream",
  "version": "0.0.1",
  "description": "Streaming, rate-limited interface to Google Maps APIs Web Services",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel ./src --out-dir ./dist",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p angular",
    "lint": "eslint ./src ./test",
    "prepublish": "npm run test && npm run build",
    "pretest": "npm run lint",
    "test": "mocha --compilers js:babel-register",
    "release": "release-it"
  },
  "keywords": [
    "api",
    "stream",
    "query",
    "limit",
    "rate",
    "rate limit",
    "query limit",
    "streaming",
    "google",
    "maps"
  ],
  "author": "Keno Schwalb <schwalb@ubilabs.net>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "conventional-changelog-cli": "^1.2.0",
    "eslint": "^3.0.1",
    "mocha": "^2.5.3",
    "release-it": "^2.4.0",
    "sinon": "^1.17.5"
  },
  "dependencies": {
    "googlemaps": "^1.11.1"
  }
}
